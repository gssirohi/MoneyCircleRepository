<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="company.greateapp.moneycircle" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LauncherActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ContactUs"
            android:label="@string/activity_contact_us" >
        </activity>
        <activity
            android:name=".RegisterAndSignInActivity"
            android:label="@string/activity_register_and_SignIn" >
        </activity>
	<activity
            android:name=".SetSplitAmount"
            android:label="@string/title_activity_set_split_amount" >
        </activity>
    </application>

</manifest>
